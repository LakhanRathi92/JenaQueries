
public class VariableLengthEnc_Dec {

	
	public static byte[] Encode(int length){
		byte[] encodedByteSize;
		
		do{
			encodedByteSize = length % 128;			
			length = length / 128;
			
			if(length > 0)
			{
				encodedByteSize = encodedByteSize |128;
			}
			
		} while(length > 0);
		
		
	}
	
	
	public static int Decode(){
		int index = 0;
		
		
		return index;
	}
	
}
